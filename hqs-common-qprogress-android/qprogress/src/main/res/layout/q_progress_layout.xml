<?xml version="1.0" encoding="utf-8"?>
<com.hqs.common.helper.qprogress.RootView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/transparent">

    <android.support.v7.widget.CardView
        android:id="@+id/q_progress_bg_view"
        android:layout_width="@dimen/q_circleProgressBgWH"
        android:layout_height="@dimen/q_circleProgressBgWH"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:cardCornerRadius="10dp"
        app:cardBackgroundColor="@android:color/darker_gray" >

        <com.pnikosis.materialishprogress.ProgressWheel
            android:id="@+id/q_progress_wheel"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true" />

        </android.support.v7.widget.CardView>



</com.hqs.common.helper.qprogress.RootView>
