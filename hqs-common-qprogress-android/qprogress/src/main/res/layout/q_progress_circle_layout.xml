<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.CardView
        android:layout_width="@dimen/q_circleProgressBgWH"
        android:layout_height="@dimen/q_circleProgressBgWH"
        android:layout_centerInParent="true"
        custom:cardCornerRadius="@dimen/q_circleProgressBgWHHalf">

        <com.github.lzyzsd.circleprogress.CircleProgress
            android:id="@+id/q_circle_progress"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            custom:circle_finished_color="#30b6ef"
            custom:circle_progress="20"
            custom:circle_text_color="@color/q_progressTextColor"
            custom:circle_text_size="@dimen/q_progressTextSize" />

    </android.support.v7.widget.CardView>


</RelativeLayout>
